PROGRAM IS
    arr[0:6], t
IN
    t := 1;
    
    arr[0] := 1;
    WRITE arr[0];       # 1

    arr[1] := 2;
    WRITE arr[1];       # 2

    arr[2] := t;
    WRITE arr[2];       # 1

    arr[3] := arr[1];
    WRITE arr[3];       # 2

    WRITE arr[t];       # 2

    arr[4] := arr[0] + arr[1];
    WRITE arr[4];       # 3

    arr[5] := 1 + arr[0];
    WRITE arr[5];       # 2

    arr[6] := arr[0] + 1;
    WRITE arr[6];       # 2
END