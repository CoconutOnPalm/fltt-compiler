PROCEDURE div_test(I x, I y) IS
    d
IN
    d := x / y;
    WRITE d;
END



PROGRAM IS
    x, y, d, arr[0:1], a, b
IN
    x := 0; y := 0;
    div_test(x, y); # 0
    x := 0; y := 1;
    div_test(x, y); # 0
    x := 1; y := 0;
    div_test(x, y); # 0
    x := 1; y := 1;
    div_test(x, y); # 1

    x := 13;
    y := 5;
    d := x / y;
    WRITE d;        # 2
    
    arr[0] := 13;
    arr[1] := 5;
    arr[0] := arr[0] / arr[1];
    WRITE arr[0];   # 2
    
    x := 10;
    y := 20;
    d := x / y;
    WRITE d;        # 0

    # big div
    x := 1152921504606846976;
    y := 1024;
    x := x * y;
    y := 1048576;
    x := x / y;
    WRITE x; # 1125899906842624

    a := 6;
    b := 3;
    # imm / imm
    x := 6 / 3;
    WRITE x;        # 2
    # imm / id
    x := 6 / b;
    WRITE x;        # 2
    # id / imm
    x := a / 3;
    WRITE x;        # 2
    # id / id
    x := a / b;
    WRITE x;        # 2
END