PROCEDURE mod_test(I x, I y) IS
    d
IN
    d := x % y;
    WRITE d;
END



PROGRAM IS
    x, y, d, arr[0:1], a, b
IN
    x := 0; y := 0;
    mod_test(x, y); # 0
    x := 0; y := 1;
    mod_test(x, y); # 0
    x := 1; y := 0;
    mod_test(x, y); # 0
    x := 1; y := 1;
    mod_test(x, y); # 0

    x := 13;
    y := 5;
    d := x % y;
    WRITE d;        # 3
    
    arr[0] := 13;
    arr[1] := 5;
    arr[0] := arr[0] % arr[1];
    WRITE arr[0];   # 3
    
    x := 10;
    y := 20;
    d := x % y;
    WRITE d;        # 10

    # big mod
    x := 1152921504606846976;
    y := 1024;
    x := x * y;
    y := 561;
    x := x % y;
    WRITE x;        # 166

    a := 6;
    b := 2;
    # imm % imm
    x := 6 % 2;
    WRITE x;        # 0
    # imm % id
    x := 6 % b;
    WRITE x;        # 0
    # id % imm
    x := a % 2;
    WRITE x;        # 0
    # id % id
    x := a % b;
    WRITE x;        # 0

    a := 11;
    b := 2;
    # imm % imm
    x := 11 % 2;
    WRITE x;        # 1
    # imm % id
    x := 11 % b;
    WRITE x;        # 1
    # id % imm
    x := a % 2;
    WRITE x;        # 1
    # id % id
    x := a % b;
    WRITE x;        # 1
END