PROGRAM IS
    x, y, arr[0:1], a, b
IN
    x := 5;
    y := 3;
    x := x * y;
    WRITE x;

    arr[0] := 4;
    arr[1] := 0;
    x := arr[0] * arr[1];
    WRITE x;

    x := 1000000000000;
    y := 1000000000000;
    arr[0] := x * y;
    WRITE arr[0];
    
    a := 2;
    b := 3;
    # imm * imm
    x := 2 * 3;
    WRITE x;        # 6
    # imm * id
    x := 2 * b;
    WRITE x;        # 6
    # id * imm
    x := a * 3;
    WRITE x;        # 6
    # id * id
    x := a * b;
    WRITE x;        # 6
    # a * b > uint64_t::max
    x := 1000000000000 * 1000000000000;
    WRITE x;        # 1000000000000 * 1000000000000

END