PROCEDURE sum(T arr, I beg, I end, O result) IS
    sum
IN
    sum := 0;
    FOR i FROM beg TO end DO
        sum := sum + arr[i];
    ENDFOR
    result := sum;
END

PROCEDURE add(I a, I b, O result) IS
IN
    result := a + b;
END

PROGRAM IS
    arr[0:100], a, b, x, beg, end
IN
    FOR i FROM 0 TO 100 DO
        arr[i] := i;
    ENDFOR

    READ a;
    READ b;
    READ beg;
    READ end;
    x := a + b;
    WRITE x;
    add(a, b, x);
    WRITE x;
    sum(arr, beg, end, x);
    WRITE x;

    a := 1;
    b := 3;
    # imm + imm
    x := 1 + 3;
    WRITE x;        # 4
    # imm + id
    x := 1 + b;
    WRITE x;        # 4
    # id + imm
    x := a + 3;
    WRITE x;        # 4
    # id + id
    x := a + b;
    WRITE x;        # 4
END