PROCEDURE foo(I n, T arr) IS
IN
    FOR i FROM 2 TO n DO
        arr[i] := i;
        WRITE arr[i];
    ENDFOR
END

PROCEDURE bar(T arr) IS
IN
    FOR i FROM 4 DOWNTO 2 DO
        arr[i] := 10;
    ENDFOR
    
    FOR i FROM 4 DOWNTO 2 DO
        WRITE arr[i];
    ENDFOR
END

PROGRAM IS
    arr[0:9], n, vec[2:4]
IN
    FOR i FROM 0 TO 9 DO
        arr[i] := i;
    ENDFOR

    FOR i FROM 0 TO 9 DO
        WRITE arr[i];
    ENDFOR
    
    FOR i FROM 9 DOWNTO 0 DO
        arr[i] := 9 - i;
    ENDFOR

    FOR i FROM 0 TO 9 DO
        WRITE arr[i];
    ENDFOR

    n := 9;
    foo(n, arr);
    bar(vec);

    FOR i FROM 0 TO 0 DO
        WRITE 1;
    ENDFOR

    FOR i FROM 0 DOWNTO 0 DO
        WRITE 1;
    ENDFOR
END
