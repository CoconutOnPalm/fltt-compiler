# Min and Max Finder
# Reads n numbers and finds minimum and maximum values

PROGRAM IS
  n, count, num, min, max
IN
  READ n;
  READ num;
  min:=num;
  max:=num;
  count:=1;
  WHILE count<n DO
    READ num;
    IF num<min THEN
      min:=num;
    ENDIF
    IF num>max THEN
      max:=num;
    ENDIF
    count:=count+1;
  ENDWHILE
  WRITE min;
  WRITE max;
END
